<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageProjectUrl>https://github.com/raerae1616/CommandLineArgsHelper</PackageProjectUrl>
    <Copyright>Copyright Â©  2018</Copyright>
    <Authors>Brandon Bernard</Authors>
    <Company>Brandon Bernard</Company>
    <Version>1.0.0.4</Version>
    <AssemblyVersion>1.0.0.4</AssemblyVersion>
    <FileVersion>1.0.0.4</FileVersion>
    <PackageTags>command line commandline arg arguments param parameters console consoleapp</PackageTags>
    <PackageReleaseNotes>Migrate assembly to target .Net Standard 2.0 for greater compatibility with .Net Framework 4.7.2+ and .Net Core 2.0+; No functionality changes.</PackageReleaseNotes>
